generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model projects {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt  DateTime @db.Date
  imageUrl   String
  name       String
  websiteUrl String
}

model video {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  title   String
  videoId String
}
